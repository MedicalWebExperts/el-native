{
  "name": "el-native",
  "version": "1.0.1",
  "private": true,
  "description": "Just Another React Native Components Library.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "TEST=true jest --watch --silent",
    "test:coverage": "TEST=true jest --coverage --silent",
    "lint": "eslint src",
    "build": "babel src --out-dir dist --ignore '**/*.test.js'",
    "build:watch": "babel src -w -d dist",
    "prepublish": "npm run build",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "updateSnapshots": "jest --updateSnapshot",
    "updateTests": "npm run test -- -u",
    "storybook": "storybook start -p 7007"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/react-native": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "1.9.2",
    "babel-preset-react-native-stage-0": "1.0.1",
    "enzyme": "2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-react-native": "2.3.2",
    "husky": "0.13.4",
    "jest": "20.0.4",
    "jest-enzyme": "3.8.3",
    "jest-serializer-enzyme": "1.0.0",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "^16.0.0",
    "react-native": "0.50.1",
    "react-test-renderer": "16.0.0"
  },
  "peerDependencies": {
    "react": "16.0.0",
    "react-native": "0.50.1",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jest/setup.js"
    ],
    "globals": {
      "__DEV__": true
    },
    "snapshotSerializers": [
      "./node_modules/jest-serializer-enzyme"
    ],
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native)"
    ]
  }
}
