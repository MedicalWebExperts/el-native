{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": [0],
    "react/forbid-prop-types": [0, { "forbid": ["object"] }]
  },
  "globals": {
    "__DEV__": true
  },
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  }
}
